{
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./api/**/*.js": [
      "prettier --write --print-width 100 --single-quote --trailing-comma all",
      "eslint --fix",
      "git add"
    ],
    "./frontend/**/*.js": [
      "git add"
    ],
    "./frontend/**/*.vue": [
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "prettier": "1.14.2"
  },
  "dependencies": {}
}
